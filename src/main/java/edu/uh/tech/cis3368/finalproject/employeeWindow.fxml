<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="572.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uh.tech.cis3368.finalproject.employeeWindowController">
   <children>
      <Button fx:id="addButton" layoutX="218.0" layoutY="248.0" mnemonicParsing="false" onAction="#addEmployee" prefHeight="27.0" prefWidth="136.0" text="Add Employee" />
      <Button fx:id="removeButton" layoutX="218.0" layoutY="284.0" mnemonicParsing="false" onAction="#removeEmployee" prefHeight="27.0" prefWidth="136.0" text="Remove Employee" />
      <TableView fx:id="table" layoutX="8.0" layoutY="27.0" prefHeight="200.0" prefWidth="556.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="77.0" sortType="DESCENDING" text="ID" />
          <TableColumn fx:id="nameCol" editable="false" prefWidth="169.0" sortable="false" text="Name" />
            <TableColumn fx:id="positionCol" editable="false" prefWidth="125.0" sortable="false" text="Position" />
            <TableColumn fx:id="rateCol" prefWidth="85.0" sortable="false" text="Rate" />
            <TableColumn fx:id="dateCol" prefWidth="99.0" sortable="false" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="raiseButton" layoutX="218.0" layoutY="318.0" mnemonicParsing="false" onAction="#giveRaise" prefHeight="27.0" prefWidth="136.0" text="Give 5% Raise" />
      <Button fx:id="editButton" layoutX="218.0" layoutY="352.0" mnemonicParsing="false" onAction="#editInfo" prefHeight="27.0" prefWidth="136.0" text="Edit" />
   </children>
</AnchorPane>
